<fx:root prefHeight="609.0" prefWidth="905.0" styleClass="workspace" stylesheets="@ideCSS.css" type="BorderPane" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
    
